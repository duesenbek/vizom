{
  "name": "vizom",
  "version": "2.0.0",
  "description": "AI-powered data visualization tool with modular architecture",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"node server.js\" \"npm:dev:css\"",
    "dev:server": "node server.js",
    "dev:vite": "vite",
    "start": "node server.js",
    "build": "vite build",
    "dev:react": "vite --mode development --open /index-react.html",
    "build:react": "cross-env REACT_BUILD=true vite build",
    "build:css": "tailwindcss -o dist/output.css",
    "dev:css": "tailwindcss -o dist/output.css --watch",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:controls": "playwright test tests/ui-controls.spec.js",
    "test:smoke": "playwright test tests/smoke/",
    "test:smoke:generator": "playwright test tests/smoke/generator.smoke.spec.js",
    "test:smoke:auth": "playwright test tests/smoke/auth.smoke.spec.js",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.{js,css,html}\"",
    "generate-previews": "node scripts/generate-chart-previews-simple.js",
    "generate-previews-full": "node scripts/generate-chart-previews.js"
  },
  "keywords": [
    "visualization",
    "charts",
    "ai",
    "deepseek",
    "chart.js",
    "modular"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.17.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/cli": "^4.1.16",
    "@tailwindcss/postcss": "^4.1.17",
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "husky": "^9.1.7",
    "jsdom": "^22.1.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.1.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^4.1.16",
    "vite": "^5.0.10",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^1.0.4"
  },
  "dependencies": {
    "@sentry/browser": "^10.25.0",
    "@sentry/tracing": "^7.120.4",
    "@supabase/supabase-js": "^2.45.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "apexcharts": "^3.54.1",
    "canvas": "^2.11.2",
    "canvas2svg": "^1.0.16",
    "chart.js": "^4.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "echarts": "^5.6.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "plotly.js-dist": "^2.35.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stripe": "^14.19.0",
    "svg2pdf.js": "^2.6.0",
    "typescript": "^5.9.3"
  }
}
