{
  "name": "vizom",
  "version": "2.0.0",
  "description": "AI-powered data visualization tool with modular architecture",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"node server.js\" \"npm:dev:css\"",
    "dev:server": "node server.js",
    "dev:vite": "vite",
    "start": "node server.js",
    "build": "vite build",
    "build:css": "tailwindcss -o dist/output.css",
    "dev:css": "tailwindcss -o dist/output.css --watch",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.{js,css,html}\""
  },
  "keywords": ["visualization", "charts", "ai", "deepseek", "chart.js", "modular"],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.17.0"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.16",
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "postcss": "^8.5.6",
    "prettier": "^3.1.1",
    "tailwindcss": "^4.1.16",
    "vite": "^5.0.10",
    "vitest": "^1.0.4"
  },
  "dependencies": {
    "chart.js": "^4.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "@supabase/supabase-js": "^2.45.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "stripe": "^14.19.0"
  }
}
