{
  "name": "vizom",
  "version": "1.0.0",
  "description": "AI-Powered Data Visualization Platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.config.prod-fixed.js",
    "build:analyze": "vite build --config vite.config.prod-fixed.js && open dist/stats.html",
    "build:dev": "vite build --mode development",
    "preview": "vite preview --port 4173",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "deploy:staging": "npm run build && npm run deploy:staging:netlify",
    "deploy:production": "npm run build && npm run deploy:production:netlify",
    "deploy:staging:netlify": "netlify deploy --dir=dist --site=vizom-staging",
    "deploy:production:netlify": "netlify deploy --dir=dist --site=vizom-production --prod",
    "optimize-images": "imagemin src/assets/images/* --out-dir=src/assets/images/optimized",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "security-audit": "npm audit --audit-level=moderate",
    "performance-audit": "lighthouse https://your-domain.com --output=json --output-path=./lighthouse-report.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "chart.js": "^4.4.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^2.30.0",
    "lucide": "^0.294.0",
    "clsx": "^2.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@vitejs/plugin-pwa": "^0.16.4",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "terser": "^5.20.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^0.34.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "ai",
    "data-visualization",
    "charts",
    "analytics",
    "deepseek",
    "chart.js",
    "dashboard"
  ],
  "author": "Vizom Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/vizom.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/vizom/issues"
  },
  "homepage": "https://vizom.com"
}
